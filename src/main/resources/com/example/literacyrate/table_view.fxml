<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.effect.Blend?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.literacyrate.TableViewController">
   <children>
      <BorderPane prefHeight="400.0" prefWidth="600.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <center>
            <TableView fx:id="tableView" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
              <columns>
                  <TableColumn fx:id="idColumn" prefWidth="75.0" text="ID" />
                <TableColumn fx:id="stateColumn" prefWidth="75.0" text="State" />
                <TableColumn fx:id="yearColumn" prefWidth="75.0" text="Year" />
                  <TableColumn fx:id="moneySpendColumn" prefWidth="75.0" text="MoneySpend" />
                  <TableColumn fx:id="educationLevelColumn" prefWidth="75.0" text="Education Level" />
                  <TableColumn fx:id="genderColumn" prefWidth="75.0" text="Gender" />
              </columns>
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
               <padding>
                  <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
               </padding>
               <BorderPane.margin>
                  <Insets />
               </BorderPane.margin>
            </TableView>
         </center>
         <bottom>
            <Button alignment="CENTER" contentDisplay="BOTTOM" mnemonicParsing="false" onAction="#createDataView" text="Graph View" textAlignment="RIGHT" BorderPane.alignment="CENTER">
               <effect>
                  <Blend />
               </effect></Button>
         </bottom>
         <top>
            <Label onDragDetected="#createDataView" text="Table Data" BorderPane.alignment="CENTER">
               <font>
                  <Font name="Elephant Italic" size="26.0" />
               </font>
            </Label>
         </top>
      </BorderPane>
   </children>
   <padding>
      <Insets top="20.0" />
   </padding>
</AnchorPane>
